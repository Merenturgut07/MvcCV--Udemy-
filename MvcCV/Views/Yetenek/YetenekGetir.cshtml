@model MvcCV.Models.Entity.TblYeteneklerim
@{
    ViewBag.Title = "YetenekGetir";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}



<br />
<br />
<h4> Yetenek Güncelleme Sayfası</h4>
<br />
<br />

<form method="post">
    @Html.Label("Yetenek Açıklaması")
    @Html.TextBoxFor(x => x.Yetenek, new { @class = "form-control" })
    <br />
    @Html.Label("Oran")
    @Html.TextBoxFor(x => x.Oran, new { @class = "form-control" })
    <br />
    <br />
    <button class="btn btn-success">Güncelle </button>

</form>
